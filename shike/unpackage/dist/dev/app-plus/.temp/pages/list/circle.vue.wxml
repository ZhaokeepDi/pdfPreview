<template name="d9cc654a">
  <view class="_view data-v-5fc0909f content column">
    <view class="_view data-v-5fc0909f center">
      <text class="_text data-v-5fc0909f">兰花广场</text>
      <view class="_view data-v-5fc0909f serch">
        <input type="text" placeholder="搜索食物" class="_input data-v-5fc0909f" />
      </view>
    </view>
    <view style="background:red" class="_view data-v-5fc0909f">
      <swiper class="_swiper data-v-5fc0909f swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"></swiper>
    </view>
    <view class="_view data-v-5fc0909f food-type" style="background:#8F8F94">
      <view wx:for="{{indexData.typeList}}" wx:for-index="index" wx:for-item="item" class="_view data-v-5fc0909f">
        <txst class="_txst data-v-5fc0909f">{{item.name}}</txst>
      </view>
    </view>
    <view class="_view data-v-5fc0909f column">
      <view class="_view data-v-5fc0909f center">
        <text class="_text data-v-5fc0909f">推荐商家</text>
      </view>
      <view class="_view data-v-5fc0909f space">
        <text bindtap="handleProxy" data-eventid="{{'1Se-0'}}" data-comkey="{{$k}}" class="_text data-v-5fc0909f">综合排序</text>
        <text bindtap="handleProxy" data-eventid="{{'ReA-1'}}" data-comkey="{{$k}}" class="_text data-v-5fc0909f">人气最高</text>
        <text bindtap="handleProxy" data-eventid="{{'Xw9-2'}}" data-comkey="{{$k}}" class="_text data-v-5fc0909f">距离最近</text>
      </view>
      <view class="_view data-v-5fc0909f center">
        <view hidden="{{!(groop==1)}}" class="_view data-v-5fc0909f"> 综合排序 </view>
        <view hidden="{{!(groop==2)}}" class="_view data-v-5fc0909f"> 人气最高 </view>
        <view hidden="{{!(groop==3)}}" class="_view data-v-5fc0909f"> 距离最近 </view>
      </view>
    </view>
  </view>
</template>